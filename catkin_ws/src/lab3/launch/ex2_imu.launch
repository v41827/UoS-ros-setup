<launch>
    <param name="use_sim_time" value="true" />

    <include file="$(find turtlebot3_bringup)/launch/includes/description.launch.xml">
        <arg name="model" value="burger" />
    </include>

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher"/>

    <node pkg="rviz" type="rviz" name="rviz"/>

    <node pkg="rosbag" type="play" name="player" args=" /root/catkin_ws/labs/lab3/lab3_data.bag"/>

    <node pkg="robot_localization" name="KF" type="ekf_localization_node">
        <param name="odom0" value="/odom_corrected"/>
        <param name="imu0_remove_gravitational_acceleration" value="true"/>
        <rosparam param="odom0_config">[false, false, false, false, false, false,
        true, true, true, true, true, true,
        false, false, false]</rosparam>
    </node>

    
</launch>